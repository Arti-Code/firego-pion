{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "go build",
            "type": "shell",
            "command": "go build",
            "problemMatcher": [
                "$eslint-stylish"
            ]
        },
        {
            "label": "run exe",
            "type": "shell",
            "command": "firego.exe",
            "problemMatcher": [
                "$eslint-stylish"
            ]
        }
    ]
}